# Urban Commute Assistant Environment Variables
# Generated automatically from Settings class


# Database Settings
# PostgreSQL connection string
DATABASE_URL=your_database_url_here

# SQLAlchemy database URI
SQLALCHEMY_DATABASE_URI=your_sqlalchemy_database_uri_here

# SQLAlchemy track modifications setting
SQLALCHEMY_TRACK_MODIFICATIONS=false

# PostgreSQL username
POSTGRES_USER=postgres

# PostgreSQL password
POSTGRES_PASSWORD=postgres

# PostgreSQL database name
POSTGRES_DB=urban_commute


# API Keys
# OpenWeatherMap API key
WEATHER_API_KEY=your_weather_api_key_here

# TomTom API key
TRAFFIC_API_KEY=your_traffic_api_key_here

# Transit API key (if needed)
TRANSIT_API_KEY=None


# Transit Data Settings
# King County Metro GTFS static data URL
KC_METRO_GTFS_URL=https://kingcounty.gov/~/media/depts/metro/schedules/gtfs/current-feed.zip

# King County Metro GTFS-RT data URL
KC_METRO_GTFS_RT_URL=https://api.pugetsound.onebusaway.org/api/where


# Redis Settings
# Redis connection string
REDIS_URL=redis://localhost:6379/0


# Security Settings
# Secret key for JWT tokens
SECRET_KEY=your_secret_key_here


# Other Settings
# Notification service URL
NOTIFICATION_SERVICE_URL=http://localhost:5000/notifications


# CORS Settings
# Comma-separated list of allowed origins or * for all
CORS_ORIGINS=*


# Logging Settings
# Logging level
LOG_LEVEL=INFO

