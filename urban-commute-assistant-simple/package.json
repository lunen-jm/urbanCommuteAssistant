{
  "name": "urban-commute-assistant-simple-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev:frontend": "npm run dev --workspace=frontend",
    "dev:backend": "npm run dev --workspace=backend",
    "dev": "npm-run-all --parallel dev:frontend dev:backend"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
